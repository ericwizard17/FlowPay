{
    "extends": "expo/tsconfig.base",
    "compilerOptions": {
        "strict": false,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "resolveJsonModule": true,
        "jsx": "react-native",
        "moduleResolution": "node",
        "allowSyntheticDefaultImports": true,
        "forceConsistentCasingInFileNames": true,
        "noImplicitAny": false,
        "strictNullChecks": false,
        "lib": [
            "ES2015",
            "ES2020"
        ]
    },
    "include": [
        "**/*.ts",
        "**/*.tsx",
        ".expo/types/**/*.ts",
        "expo-env.d.ts"
    ],
    "exclude": [
        "node_modules"
    ]
}