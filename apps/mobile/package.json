{
  "name": "flowpay-mobile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "android:build": "eas build --platform android --profile production",
    "android:preview": "eas build --platform android --profile preview",
    "ios": "expo run:ios",
    "ios:build": "eas build --platform ios --profile production",
    "web": "expo start --web",
    "test": "jest --coverage",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,json}\"",
    "build:preview": "eas build --platform all --profile preview",
    "build:production": "eas build --platform all --profile production",
    "submit:android": "eas submit --platform android",
    "submit:ios": "eas submit --platform ios"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native": "^6.1.9",
    "@tanstack/react-query": "^5.17.0",
    "axios": "^1.6.5",
    "date-fns": "^3.0.6",
    "expo": "~50.0.0",
    "expo-blur": "~12.9.0",
    "expo-build-properties": "~0.11.0",
    "expo-haptics": "~12.8.0",
    "expo-linear-gradient": "~12.7.0",
    "expo-local-authentication": "~13.8.0",
    "expo-secure-store": "~12.8.0",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.6",
    "react-native-chart-kit": "^6.12.0",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-reanimated": "~3.6.1",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "react-native-svg": "^14.1.0",
    "react-native-toast-message": "^2.3.3",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@babel/core": "^7.23.7",
    "@testing-library/react-native": "^12.4.3",
    "@types/react": "~18.2.45",
    "@types/react-native": "~0.73.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "private": true
}
